                    "400": {
                        "description": "4** error",
                        "schema": {
                            "$ref": "#/definitions/handler.MyError"
                        }
                    },
                    "401": {
                        "description": "4** error",
                        "schema": {
                            "$ref": "#/definitions/handler.MyError"
                        }
                    },
                    "404": {
                        "description": "4** error",
                        "schema": {
                            "$ref": "#/definitions/handler.MyError"
                        }
