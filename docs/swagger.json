            },
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Add a song to user's collection",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Songs"
                ],
                "summary": "Add a Song",
                "parameters": [
                    {
                        "description": "Song details",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/musiclib.Song"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully added",
                        "schema": {
                            "$ref": "#/definitions/handler.AddSongResponse"
                        }
